{% extends "layouts/default.njk" %}
{% block backLink %}
  {# Remove back button on this page by replacing it with nothing #}
{% endblock %}
{% block main_content %}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      <h1 class="govuk-heading-l">{{ i18n.companyInactiveTitle }}</h1>
      <p class="govuk-body">
        {{ i18n.canNotRegister | replace('{companyName}', companyName) }}
      </p>
      <p class="govuk-body">
        {{ i18n.toRegister }}
        <a class="govuk-link" href={{startPage}}>{{ i18n.startAgain }}</a>
      </p>
    </div>
  </div>
{% endblock main_content %}