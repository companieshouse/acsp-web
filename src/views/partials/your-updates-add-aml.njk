{% set ADD_ROWS = [] %}
  {% for body in addedAMLBodies %}
    {% set row = {
      key: {
        text:  AMLSupervioryBodiesFormatted[body.membershipName]
      },
      value: {
        html:  body.membershipNumber + "<br> " +  i18n.yourUpdatesStarted + " " + body.changedDate
      },
      actions: {
        items: [
          {
            href: addAMLUrl + "&update=" + loop.index0,
            text: i18n.Edit,
            visuallyHiddenText: AMLSupervioryBodiesFormatted[body.membershipName]
          },
          {
            href: removeAMLUrl + "&amlindex=" + body.membershipNumber + "&amlbody=" + body.membershipName + "&return=your-updates",
            text: i18n.Cancel,
            visuallyHiddenText: AMLSupervioryBodiesFormatted[body.membershipName]
          }
        ]
      }
    } %}
    {% set ADD_ROWS = ADD_ROWS.concat([row]) %}
  {% endfor %}

  <h2 class="govuk-heading-m">{{ i18n.yourUpdatesAddAML }}</h2>
  {{ govukSummaryList({
    rows: ADD_ROWS
  }) }}